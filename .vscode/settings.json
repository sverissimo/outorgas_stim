{
    "python.formatting.provider": "autopep8",
    "python.formatting.autopep8Args": [
        "--ignore=E402"
    ],
    "cSpell.words": [
        "apexcharts",
        "applymap",
        "cotacao",
        "Cristovao",
        "datatables",
        "dayfirst",
        "debitos",
        "fillna",
        "iloc",
        "inplace",
        "iterrows",
        "LTDA",
        "nopep",
        "pgto",
        "relatorio",
        "sicar",
        "SICAR",
        "tjlp",
        "usecols",
        "VIACAO",
        "vigencia",
        "xaxis"
    ],
    "files.exclude": {
        "**/__pycache__": true
    },
    "python.linting.pylintEnabled": false,
    "python.linting.enabled": false,
    "python.linting.mypyEnabled": true,
    "python.testing.pytestArgs": [
        ".",
        "--verbose",
        "-s"
    ],
    "python.testing.unittestEnabled": false,
    "python.testing.pytestEnabled": true,
    "jenkins-runner.jobs": {
        "test 1 - defaults": {
            "isDefault": true,
            "runWith": "host-with-password",
            "name": "tst",
        },
        "test 2 - job with params": {
            "runWith": "host-with-password",
            "name": "tst2",
            "parameters": {
                "message": "Testing custom message..."
            },
            "environment": {
                "HOST_NAME": "localhost",
                "CPU_COUNT": 3
            }
        }
    },
    "cSpell.ignoreWords": [
        "Arrecadacao",
        "carencia",
        "convalidacao"
    ]
}